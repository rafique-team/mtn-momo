{
  "name": "mtn-integration",
  "version": "0.0.1",
  "description": "MTN Ghana integration for Unumed",
  "author": "Unumed",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --force-exit --testTimeout=100000  --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.12"
  },
  "contributors": [
    "Albert Llebaria Holda <aleb@unumed.com>"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "peerDependencies": {
    "@nestjs/common": "^10.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-rewire": "^1.2.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.8",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "allure-jest": "^2.10.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-allure": "^0.1.3",
    "jest-expect-message": "^1.1.3",
    "jest-jasmine2": "^29.7.0",
    "jest-junit": "^16.0.0",
    "node-mocks-http": "^1.14.1",
    "prettier": "^3.1.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.8.1",
    "semantic-release": "^22.0.12",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "bitbucket",
    "url": "https://bitbucket.org/unumed/mtn"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "axios": "^1.6.5",
    "i": "^0.3.7",
    "joi": "^17.11.0",
    "moment": "^2.30.1",
    "npm": "^10.2.5",
    "winston": "^3.11.0"
  }
}
